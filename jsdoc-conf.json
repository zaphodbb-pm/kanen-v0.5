{
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "parser": "gfm"
  },
  "source": {
    "exclude": [
      ".cache",
      ".deploy",
      ".meteor",
      ".git",
      ".gitignore",
      "node_modules",
      "out",
      "*.spec.*",
      "*.test.*"
    ],
    "includePattern": "\\.(js|jsdoc)$",
    "excludePattern": "(^|\\/|\\\\)_"
  }
}
